  `last_month_status` varchar(32) DEFAULT NULL,
  `last_month_payment_date` tinyint(2) DEFAULT NULL,
  `current_month_ptp_count` int(11) DEFAULT '0',
  `current_month_nptp_count` int(11) DEFAULT '0',
  `current_month_fol_count` int(11) DEFAULT '0',
  `ptp_status` varchar(32) DEFAULT NULL,
  `ptp_date` date DEFAULT NULL,
