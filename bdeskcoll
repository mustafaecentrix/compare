  `full_payment_count` int(11) DEFAULT '0',
  `last_call_result` varchar(32) DEFAULT NULL,
  `last_call_status` varchar(32) DEFAULT NULL,
  `last_call_status_agent` varchar(32) DEFAULT NULL,
