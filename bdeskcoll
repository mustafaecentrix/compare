CREATE TABLE `acs_user` (
  `id` varchar(128) NOT NULL,
  `first_name` varchar(50) NOT NULL DEFAULT '',
  `last_name` varchar(50) DEFAULT NULL,
  `password` varchar(32) NOT NULL,
  `password_date` datetime DEFAULT NULL,
  `password_status` varchar(7) DEFAULT NULL,
  `group_id` varchar(32) NOT NULL,
  `group_profile_id` varchar(32) DEFAULT NULL,
  `npk` varchar(20) DEFAULT NULL,
  `cd_collector` varchar(6) NOT NULL DEFAULT '',
  `cd_sp` varchar(6) NOT NULL DEFAULT '',
  `spv_id` varchar(32) DEFAULT NULL,
  `contract_number_handling` varchar(17) DEFAULT NULL,
  `ip_address` varchar(16) DEFAULT NULL,
  `extension` char(6) DEFAULT NULL,
  `agent_shift` varchar(32) DEFAULT NULL,
  `is_login` varchar(5) DEFAULT '0',
  `last_login` datetime DEFAULT NULL,
  `is_active` varchar(5) NOT NULL DEFAULT '1',
  `login_attempts` tinyint(1) DEFAULT '0',
  `authority` text,
  `created_by` varchar(32) NOT NULL,
  `created_time` datetime NOT NULL,
  `updated_by` varchar(32) NOT NULL,
  `updated_time` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `group_id` (`group_id`),
  KEY `user_id` (`id`) USING BTREE,
  KEY `contract_number_handling_idx` (`contract_number_handling`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 ROW_FORMAT=COMPACT;
