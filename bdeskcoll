  `updated_time` datetime DEFAULT NULL,
  `group_telephony` varchar(30) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `idx_id` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE `acs_predictive_queue` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `contract_number` varchar(32) NOT NULL,
  `customer_id` varchar(32) DEFAULT NULL,
  `class_id` varchar(32) NOT NULL DEFAULT '0',
  `status` tinyint(3) unsigned DEFAULT '0',
  `current_priority` tinyint(3) unsigned DEFAULT '0',
  `attempt_count` tinyint(3) unsigned DEFAULT '0',
  `last_attempt_datetime` datetime DEFAULT NULL,
  `createdDatetime` datetime DEFAULT NULL,
  `redial_status` tinyint(3) unsigned DEFAULT '0',
  `appointment_status` tinyint(3) DEFAULT '0',
  `score` int(11) unsigned DEFAULT '0',
  `created_datetime` datetime DEFAULT NULL,
  `handPhone1` varchar(30) NOT NULL,
  `handPhone2` varchar(30) NOT NULL,
  `homePhone1` varchar(30) NOT NULL,
  `homePhone2` varchar(30) NOT NULL,
  `officePhone3` varchar(30) NOT NULL,
  `officePhone2` varchar(30) NOT NULL,
  `businessPhone1` varchar(30) NOT NULL,
  `businessPhone2` varchar(30) NOT NULL,
  `appointment_datetime` datetime DEFAULT NULL,
  `prefix_handp1` tinyint(1) DEFAULT '0',
  `prefix_handp2` tinyint(1) DEFAULT '0',
  `prefix_homep1` tinyint(1) DEFAULT '0',
  `prefix_homep2` tinyint(1) DEFAULT '0',
  `prefix_officep1` tinyint(1) DEFAULT '0',
  `prefix_officep2` tinyint(1) DEFAULT '0',
  `prefix_bussines1` tinyint(1) DEFAULT '0',
  `prefix_bussines2` tinyint(1) DEFAULT '0',
  `cycle` tinyint(2) DEFAULT '0',
  `officePhone1` varchar(30) NOT NULL,
  `ptp_date` date DEFAULT NULL,
  `flag_multigroup` varchar(1) DEFAULT NULL,
  `group_number` varchar(15) DEFAULT NULL,
  `branch_mst_id` varchar(30) DEFAULT NULL,
  `pic_phone` varchar(20) DEFAULT NULL,
  `work_shift` tinyint(4) DEFAULT '1',
  PRIMARY KEY (`id`),
  UNIQUE KEY `id` (`id`),
  KEY `id_2` (`id`,`contract_number`),
  KEY `class_id` (`class_id`),
  KEY `contract_number_idx` (`contract_number`),
  KEY `idx_status` (`status`),
  KEY `id_3` (`class_id`,`status`,`attempt_count`,`last_attempt_datetime`,`redial_status`),
  KEY `NewIndex1` (`last_attempt_datetime`),
  KEY `customer_id_idx` (`customer_id`),
  KEY `contract_customer_idx` (`contract_number`,`customer_id`),
  KEY `idx_clasid_status` (`class_id`,`status`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1;
