  `insert_time` datetime DEFAULT NULL,
  `yearmonth` varchar(6) DEFAULT NULL,
  PRIMARY KEY (`contract_number`,`customer_id`),
  KEY `contract_number_idx` (`contract_number`),
  KEY `last_call_status_idx` (`last_call_status`),
  KEY `last_call_status_agent_idx` (`last_call_status_agent`),
  KEY `last_call_status_time_idx` (`last_call_status_agent`),
  KEY `last_call_od_days_idx` (`last_call_od_days`),
  KEY `last_month_status_idx` (`last_month_status`),
  KEY `due_gerak_idx` (`due_gerak`),
  KEY `insert_time_idx` (`insert_time`),
  KEY `yearmonth_idx` (`yearmonth`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
