CREATE TABLE `acs_agent_assignment` (
  `id` varchar(32) NOT NULL DEFAULT '',
  `user_id` varchar(32) DEFAULT NULL,
  `team` varchar(32) DEFAULT NULL,
  `created_by` varchar(30) DEFAULT NULL,
  `created_time` datetime DEFAULT NULL,
  `updated_by` varchar(30) DEFAULT NULL,
  `updated_time` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `id_idx` (`id`),
  KEY `user_id_idx` (`user_id`),
  KEY `team_idx` (`team`),
  KEY `created_by_idx` (`created_by`),
  KEY `created_time_idx` (`created_time`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 ROW_FORMAT=COMPACT;
CREATE TABLE `acs_agent_script` (
  `id` varchar(32) NOT NULL,
  `subject` varchar(100) DEFAULT NULL,
  `script` text,
  `is_active` char(1) DEFAULT '1',
  `created_by` varchar(32) DEFAULT NULL,
  `created_time` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE `acs_asset_detail` (
  `contract_number` varchar(32) NOT NULL,
  `asset_type` varchar(32) DEFAULT NULL,
  `no_rangka` varchar(32) DEFAULT NULL,
  `no_mesin` varchar(32) DEFAULT NULL,
  `color` varchar(32) DEFAULT NULL,
  `license_plate` varchar(32) DEFAULT NULL,
  `year` char(4) DEFAULT NULL,
  `asset_registration_name` varchar(50) DEFAULT NULL,
  `used_new` varchar(32) DEFAULT NULL,
  `insert_time` datetime DEFAULT NULL,
  PRIMARY KEY (`contract_number`),
  KEY `contract_number_idx` (`contract_number`),
  KEY `asset_type_idx` (`asset_type`),
  KEY `insert_time_idx` (`insert_time`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE `acs_call_history_daily` (
  `id` varchar(32) NOT NULL,
  `contract_number` varchar(17) NOT NULL,
  `overdue_days` int(11) NOT NULL,
  `installment_no` int(11) DEFAULT NULL,
  `phone_number` varchar(15) NOT NULL,
  `visit_date` datetime DEFAULT NULL,
  `contact_status` varchar(32) DEFAULT NULL,
  `connect_status` varchar(32) DEFAULT NULL,
  `contact_person` varchar(30) NOT NULL,
  `call_result` varchar(32) NOT NULL,
  `follow_up` varchar(32) DEFAULT NULL,
  `escalation` varchar(32) DEFAULT NULL,
  `notepad` varchar(255) NOT NULL,
  `notepad2` varchar(255) DEFAULT NULL,
  `contact_person_name` varchar(50) DEFAULT '',
  `unit_status` varchar(32) DEFAULT NULL,
  `unit_user` varchar(32) DEFAULT NULL,
  `unit_user_name` varchar(50) DEFAULT '',
  `ptp_date` date DEFAULT NULL,
  `ptp_amount` int(11) DEFAULT NULL,
  `paid_date` date DEFAULT NULL,
  `paid_amount` int(11) DEFAULT NULL,
  `paid_charge` int(11) DEFAULT NULL,
  `payment_via` varchar(32) DEFAULT NULL,
  `visited_address` varchar(255) DEFAULT NULL,
  `aro_id` varchar(32) DEFAULT NULL,
  `call_time` datetime DEFAULT NULL,
  `created_by` varchar(32) NOT NULL,
  `created_time` datetime NOT NULL,
  `bucket_aro` varchar(32) DEFAULT NULL,
  `branch_name` varchar(32) DEFAULT NULL,
  `branch_code` int(3) DEFAULT NULL,
  `is_customer_talking` varchar(15) DEFAULT NULL,
  `caller_id` varchar(32) DEFAULT NULL,
  `recording_id` varchar(32) DEFAULT NULL,
  `recording_file` varchar(255) DEFAULT NULL,
  `cycle` tinyint(2) DEFAULT NULL,
  `class_id` varchar(32) DEFAULT NULL,
  `contact_status2` varchar(32) DEFAULT NULL,
  `call_status` varchar(32) DEFAULT NULL,
  `spcl_status` varchar(32) DEFAULT 'OPEN',
  `spcl_remark` varchar(255) DEFAULT NULL,
  `spcl_approve_by` varchar(32) DEFAULT NULL,
  `spcl_approve_date` datetime DEFAULT NULL,
  `spcl_call_attempt` tinyint(2) DEFAULT NULL,
  KEY `contract_number_idx` (`contract_number`),
  KEY `phone_number_idx` (`phone_number`),
  KEY `visit_date_idx` (`visit_date`),
  KEY `contact_person_idx` (`contact_person`),
  KEY `unit_status_idx` (`unit_status`),
  KEY `unit_user_idx` (`unit_user`),
  KEY `call_result_idx` (`call_result`),
  KEY `ptp_date_idx` (`ptp_date`),
  KEY `payment_via_idx` (`payment_via`),
  KEY `aro_id_idx` (`aro_id`),
  KEY `call_time_idx` (`call_time`),
  KEY `created_by_idx` (`created_by`),
  KEY `created_time_idx` (`created_time`),
  KEY `bucket_aro_idx` (`bucket_aro`),
  KEY `branch_code_idx` (`branch_code`),
  KEY `caller_id_idx` (`caller_id`),
  KEY `call_status_idx` (`call_status`),
  KEY `cycle_idx` (`cycle`),
  KEY `class_id_idx` (`class_id`),
  KEY `idx_Id` (`id`),
  KEY `is_customer_talking_idx` (`is_customer_talking`),
  KEY `contact_status_idx` (`contact_status`),
  KEY `connect_status_idx` (`connect_status`),
  KEY `follow_up_idx` (`follow_up`),
  KEY `escalation_idx` (`escalation`),
  KEY `spcl_status_idx` (`spcl_status`),
  KEY `spcl_approve_by_idx` (`spcl_approve_by`),
  KEY `spcl_approve_date_idx` (`spcl_approve_date`),
  KEY `spcl_call_attempt_idx` (`spcl_call_attempt`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE `acs_call_history_detail_daily` (
  `call_history_id` varchar(36) NOT NULL DEFAULT '',
  `sub_call_result` varchar(255) DEFAULT NULL,
  `note` varchar(255) DEFAULT NULL,
  `cycle` char(1) DEFAULT NULL,
  `class_id` varchar(32) DEFAULT NULL,
  `caller_id` varchar(32) DEFAULT NULL,
  `recording_dir_path` varchar(255) DEFAULT NULL,
  `dialing_mode` varchar(32) DEFAULT NULL,
  `phone_type` varchar(12) DEFAULT NULL,
  `user_id` varchar(36) DEFAULT NULL,
  `start_call` datetime DEFAULT NULL,
  `end_call` datetime DEFAULT NULL,
  `created_time` datetime DEFAULT NULL,
  `queue_status` varchar(16) DEFAULT NULL,
  PRIMARY KEY (`call_history_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE `acs_call_log` (
  `call_history_id` varchar(32) NOT NULL DEFAULT '',
  `contract_number` varchar(17) DEFAULT NULL,
  `class_id` varchar(32) DEFAULT NULL,
  `cycle` char(2) DEFAULT NULL,
  `collection_id` varchar(32) DEFAULT NULL,
  `dialing_time` datetime DEFAULT NULL,
  `connect_time` datetime DEFAULT NULL,
  `disconnect_time` datetime DEFAULT NULL,
  `save_time` datetime DEFAULT NULL,
  `call_result` varchar(32) DEFAULT NULL,
  `user_id` varchar(32) DEFAULT NULL,
  `created_time` datetime DEFAULT NULL,
  PRIMARY KEY (`call_history_id`),
  KEY `contract_number_idx` (`contract_number`),
  KEY `class_id_idx` (`class_id`),
  KEY `cycle_idx` (`cycle`),
  KEY `collection_id_idx` (`collection_id`),
  KEY `dialing_time_idx` (`dialing_time`),
  KEY `connect_time_idx` (`connect_time`),
  KEY `disconnect_time_idx` (`disconnect_time`),
  KEY `save_time_idx` (`save_time`),
  KEY `call_result_idx` (`call_result`),
  KEY `created_time_idx` (`created_time`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC;
